{
  "expo": {
    "name": "redoo",
    "slug": "redoo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logos/icon.png",
    "scheme": "redoo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logos/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f172a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mendessamacriano.redoo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logos/adaptive-icon.png",
        "backgroundColor": "#0f172a"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e7c31089-7bba-4cac-82cf-8c962717c692"
      }
    }
  }
}
